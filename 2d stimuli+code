Stimuli and Code for calculating valence and arousal ratings for Musical stimuli taken from Eerola and Vuoskoksi (2011) 

MUVA = VALENCE RATING 
MUAR = AROUSAL RATING 
#SAD MUSIC 
  STIMULI USED 
    MU31 
    MU86
    MU88
    MU109
   R CODE 
   #Valece
   mydata$MUVASAD <- (mydata$MUVA31 + mydata$MUVA86 + mydata$MUVA88 + mydata$MUVA109) / 4
   #AROUSAL 
   mydata$MUARSAD <- (mydata$MUAR31 + mydata$MUAR86 + mydata$MUAR88 + mydata$MUAR109) / 4

#ANGRY MUSIC 
  STIMULI USED 
    MU1
    MU2
    MU3
    MU69
   R CODE
   #Valence 
   mydata$MUVAANGRY <- (mydata$MUVA1 + mydata$MUVA2 +mydata$MUVA3 + mydata$MUVA69) / 4
   #AROUSAL
   mydata$MUARANGRY <- (mydata$MUAR1 + mydata$MUAR3 + mydata$MUAR3 + mydata$MUAR69) / 4
   
 #FEAR MUSIC 
  STIMULI USED
    MU11
    MU13
    MU14
    MU15
   R CODE
   #VALENCE
   mydata$MUVAFEAR <- (mydata$MUVA11 + mydata$MUVA13 + mydata$MUVA14 + mydata$MUVA15) / 4
   #AROUSAL
   mydata$MUARFEAR <- (mydata$MUAR11 + mydata$MUAR13 + mydata$MUAR14 + mydata$MUAR15) / 4
   
   #HAPPY MUSIC 
    STIMULI 
      MU21
      MU22
      MU23
      MU105
     R CODE
     #VALENCE
     mydata$MUVAHAPPY <- (mydata$MUVA21 + mydata$MUVA22 + mydata$MUVA23 + mydata$MUVA105) / 4 
     #AROUSAL
     mydata$MUARHAPPY <- (mydata$MUAR21 + mydata$MUAR22 + mydata$MUAR23 + mydata$MUAR15) / 4
